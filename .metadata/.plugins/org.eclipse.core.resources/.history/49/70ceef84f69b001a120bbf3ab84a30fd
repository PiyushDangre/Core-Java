package app;

class MyClass {
	
	public Object obj ;
	
	public Object getObj() {
		return this.obj;
	}
	
	public void setObj(Object o) {
		this.obj = o ;
	}
	
}

public class myApp {

	public static void main(String args[]) {
		MyClass m = new MyClass();
		m.setObj(45);
		Integer i = (Integer) m.getObj() ;
		
	}
	
}
